<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>3.4.5</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>com.wellalmeida31</groupId>
	<artifactId>redshift-client</artifactId>
	<version>0.0.2</version>
	<name>redshift-client</name>
	<description>Interface JDBC de conex√£o com redshift</description>
	<packaging>jar</packaging>
	<url/>
	<licenses>
		<license/>
	</licenses>
	<developers>
		<developer/>
	</developers>
	<scm>
		<connection/>
		<developerConnection/>
		<tag>library</tag>
		<url>https://github.com/WellAlmeida31/redshift-client/tree/master/</url>
	</scm>

	<properties>
		<java.version>21</java.version>
		<maven.deploy.skip>false</maven.deploy.skip>
		<http.socket.timeout>60000</http.socket.timeout>
		<http.connection.timeout>60000</http.connection.timeout>
	</properties>

	<distributionManagement>
		<repository>
			<id>github</id>
			<name>Redshift Functional JDBC Connector</name>
			<url>https://maven.pkg.github.com/WellAlmeida31/redshift-client</url>
		</repository>
	</distributionManagement>

	<dependencies>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-web</artifactId>
		</dependency>
		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<version>1.18.30</version>
		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-data-jpa</artifactId>
		</dependency>

		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-test</artifactId>
			<scope>test</scope>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
			<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-deploy-plugin</artifactId>
			<version>3.1.3</version>
			<configuration>
				<groupId>com.wellalmeida31</groupId>
				<artifactId>redshift-client</artifactId>
				<repositoryId>github</repositoryId>
				<url>https://maven.pkg.github.com/WellAlmeida31/redshift-client</url>
			</configuration>
		</plugin>
		</plugins>
	</build>

</project>
